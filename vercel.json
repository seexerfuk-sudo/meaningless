{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/brat",
      "destination": "/brat.html"
    },
    {
      "source": "/yapper",
      "destination": "/yapper.html"
    },
    {
      "source": "/art",
      "destination": "/art.html"
    },
    {
      "source": "/unsent",
      "destination": "/card.html"
    },
    {
      "source": "/card",
      "destination": "/card.html"
    },
    {
      "source": "/(.*)\\.html",
      "destination": "/$1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
